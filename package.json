{
  "name": "vfs-dac-library",
  "version": "1.1.0",
  "description": "Data access control npm package for VFS data portal",
  "author": "Ruben Bernardes",
  "license": "MIT",
  "repository": "Ruben Bernardes/vfs-dac-library",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --css-modules false --no-compress --format modern,cjs",
    "start": "microbundle-crl --css-modules false watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "refreshVSToken": "vsts-npm-auth -config .npmrc",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@azure/msal-browser": "^2.14.1",
    "@material-ui/core": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "axios": "^0.21.1",
    "node-sass": "^5.0.0",
    "vfs-msal-auth": "^1.0.8"
  }
}
